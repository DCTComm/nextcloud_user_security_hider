<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
	<id>user_security_hider</id>
	<name>User Security Hider</name>
	<summary>Hide user information from other users</summary>
	<description>Hide user information from other users</description>
	<version>1.0.0</version>
	<licence>agpl</licence>
	<author mail="danish.ahmed@dctcommunication.com" homepage="https://dctcommunication.com">Danish</author>
	<namespace>UserSecurityHider</namespace>
	<category>customization</category>
	<bugs>https://github.com/dctcommunication/user_security_hider/issues</bugs>
	<dependencies>
		<nextcloud min-version="29" max-version="31"/>
	</dependencies>

	<types>
		<logging/>
		<middleware/>
		<prevent_group_restriction/>
	</types>

	<background-jobs>
		<job>OCA\UserSecurityHider\AppInfo\Application</job>
	</background-jobs>

	<repair-steps>
		<install>
			<step>OCA\UserSecurityHider\Migration\InstallRepairStep</step>
		</install>
		<post-migration>
			<step>OCA\UserSecurityHider\Migration\InstallRepairStep</step>
		</post-migration>
	</repair-steps>
